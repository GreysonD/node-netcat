{
  "name": "node-netcat",
  "version": "2.0.0-1",
  "description": "nc/netcat - arbitrary TCP and UDP connections and listens in node.js",
  "main": "index.js",
  "scripts": {
    "test": "lab -v -c",
    "jshint": "jshint -c .jshintrc lib/*.js test/*.js",
    "code-style": "jscs -p google lib/*.js test/*.js",
    "check-coverage": "lab -t 100 -r html -o coverage.html",
    "open-coverage": "open coverage.html"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joaquimserafim/node-netcat"
  },
  "keywords": [
    "netcat",
    "nc",
    "tcp",
    "udp",
    "portscan"
  ],
  "author": "@joaquimserafim",
  "license": "ISC",
  "devDependencies": {
    "code": "^1.4.0",
    "istanbul": "^0.3.13",
    "jscs": "^1.12.0",
    "jshint": "^2.7.0",
    "lab": "^5.7.0",
    "pre-commit": "^1.0.6",
    "tape": "^4.0.0"
  },
  "dependencies": {
    "is-js-object": "^1.0.0",
    "lasync": "^1.1.0",
    "underscore": "^1.7.0"
  },
  "pre-commit": [
    "jshint",
    "code-style",
    "test",
    "check-coverage"
  ]
}
